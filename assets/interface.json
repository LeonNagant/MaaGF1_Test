    "弹药检查版本选择_8_1N": {
      "cases": [
        {
          "name": "启用新版弹药识别"
        },
        {
          "name": "启用旧版弹药识别",
          "pipeline_override": {
            "狗粮队确定_8_1N部署": {
              "recognition": "OCR",
              "roi": [
                933,
                556,
                180,
                100
              ],
              "expected": [
                "确定",
                "取消"
              ],
              "action": "Click",
              "post_delay": 1200,
              "next": [
                "81N部署_弹药检查"
              ]
            },
            "布置完成_8_1N部署": {
              "recognition": "TemplateMatch",
              "roi": [
                179,
                78,
                277,
                233
              ],
              "template": "combat/levelUp/8-1N/map_team2.png",
              "rate_limit": 200,
              "next": [
                "81N部署_弹药检查"
              ]
            },
            "!81N总流程": {
              "recognition": "OCR",
              "expected": [
                "巨鹰与寒鸦",
                "开始作战"
              ],
              "next": [
                "81N进入_点击8-1N",
                "81N部署_部署梯队",
                "81N部署_弹药检查"
              ],
              "interrupt": [
                "81N地图初始化_拖到左上角"
              ]
            }
          }
        }
      ]
    },
    "弹药检查版本选择_134": {
      "cases": [
        {
          "name": "启用新版弹药识别"
